.navs{
  width: 100%;
  background-color: #F4F4F4;
}
.page-body{
  padding: 30rpx 0 30rpx 0;
  margin: 0 30rpx;
  background-color: #f4f4f4;
}
.weui-input{
  height: 70rpx;
  padding-left: 60rpx;
  box-sizing: border-box;
  border-radius: 100rpx;
  background-color: #fff;
}
.weui-cell {
  position: relative;
}
.weui-cell_input{
  position: relative;
}
.weui-cell>image{
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  top: 20rpx;
  left: 23rpx;
}
.swiper{
  height: 200rpx;
  margin: 0 15rpx;
}
.swiper-item{
  box-sizing: border-box;
  width: 690rpx;
  height: 200rpx !important;
}
.swiper-item>image{
  width: 690rpx;
  /* height: 200rpx; */
  height: 100%;
  border-radius: 20rpx;
}
.picture{
  padding: 30rpx;
  display: flex;
  justify-content: space-between;
  position: relative;
}
.picture image{
  width: 330rpx;
  height: 120rpx;
  border-radius: 20rpx;
}
.classic{
  font-size: 28rpx;
  color: #402c2e;
  position: absolute;
  left: 414rpx;
  top: 55rpx;
}
.hotattack{
  font-size: 24rpx;
  color: #402c2e;
  position: absolute;
  left: 420rpx;
  top: 100rpx;
}
.container{
  background: linear-gradient(135deg, rgb(255, 237, 236) 0%, rgb(255, 255, 255) 100%);
  color: #222;
  height: 92rpx !important;
  line-height: 92rpx;
  padding: 0;
  display: flex;
  flex-direction: row;
  justify-content: left;
  border-top-right-radius: 20rpx;
  border-top-left-radius: 20rpx;
}
.container>text{
  padding-left: 20rpx;
  font-size: 30rpx;
}
.container>text:nth-child(2){
  color: violet;
}
.limited{
  margin: 0 30rpx 30rpx 30rpx;
  border-radius: 20rpx;
  height: 500rpx;
  background-color: #fff;
}
.limited-top{
  padding: 24rpx 24rpx 0;
  height: 408rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}
.bluetooth{
  width: 200rpx;
  height: 354rpx;
  border: 1rpx solid #F5F5F6;
  border-radius: 10rpx;
}
.bluetooth image{
  width: 200rpx;
  height: 200rpx;
  border-top-right-radius: 10rpx;
  border-top-left-radius: 10rpx;
}
.txt{
  width: 176rpx;
  height: 74rpx;
  margin: 12rpx;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
}
.price{
  color: #ed3934;
  font-size: 30rpx;
  margin-left: 12rpx;
}
.price>text{
  color: #aaaaaa;
  font-size: 24rpx;
  text-decoration: line-through;
}
.unicorn{
  height: 500rpx;
  margin: 0 30rpx 0 30rpx;
  background-color: #fff;
  border-radius: 20rpx;
}
.unicorn-top{
  height: 92rpx;
  line-height: 92rpx;
  background: linear-gradient(135deg, rgb(242, 238, 255) 0%, rgb(255, 255, 255) 100%);
  padding: 0 20rpx 0 20rpx;
  border-top-right-radius: 20rpx;
  border-top-left-radius: 20rpx;
  display: flex;
  justify-content: space-between;
}
.unicorn-top>text:nth-of-type(1){
  font-size: 30rpx;
  color: #333;
}
.unicorn-more{
  font-size: 24rpx;
  color: #aaaaaa;
}